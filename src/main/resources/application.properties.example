spring.application.name=guestApp

#Postgresql setup
spring.datasource.url=jdbc:postgresql://localhost:5432/your_db_name
spring.datasource.username=
spring.datasource.password=
spring.datasource.driverClassName=org.postgresql.Driver
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
server.port=

# JWT secret key (base64 encoded and at least 256 bits, i.e., 32 bytes or more)
security.jwt.secret-key=

# Expiration times in milliseconds (e.g., 15 minutes for access, 7 days for refresh)
security.jwt.access-token-expiration=
security.jwt.refresh-token-expiration=
